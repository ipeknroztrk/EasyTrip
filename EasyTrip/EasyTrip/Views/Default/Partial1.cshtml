@using EasyTrip.Models.Classes
@model List<Blog>
<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">

        @{  int i = 0;}
        @foreach (var İTEM in Model)
        {
            <li class="list-bg"><span>@{ i++;} @i.</span><a href="/Blog/BlogDetail/@İTEM.BlogId">@İTEM.Title</a></li>
        }


    </ul>
    <a href="#" class="link1">Tümünü Gör</a>
</div>
